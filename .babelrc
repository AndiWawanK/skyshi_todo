{
    "plugins": [
      [
        "module-resolver",
        {
          "cwd": "babelrc",
          "root": ["./src"],
          "extensions": [".js", ".ios.js", ".android.js"],
          "alias": {
            "assets": "./src/assets",
            "components": "./src/components",
            "constants": "./src/constants",
            "navigations": "./src/navigations",
            "scenes": "./src/scenes",
            "styles": "./src/styles",
            "utils": "./src/utils"
          }
        }
      ],
      ["module:react-native-dotenv", {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "blocklist": null,
        "allowlist": null,
        "safe": false,
        "allowUndefined": true,
        "verbose": false
      }]
    ]
  }